{
    "name": "makinacorpus/drupal-cryptfield",
    "type": "drupal-module",
    "description": "Encrypt field values at the field sql storage level",
    "homepage": "http://github.com/makinacorpus/drupal-cryptfield",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Pierre Rineau",
            "email": "pierre.rineau@makina-corpus.com"
        },
        {
            "name": "Makina Corpus",
            "homepage": "http://makina-corpus.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MakinaCorpus\\Drupal\\Sf\\" : "src/"
        },
        "psr-0": {
            "Drupal\\" : "lib/"
        },
        "files": [
            "lib/Drupal.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^5.1"
    },
    "bin": [
        "bin/console"
    ],
    "require": {
        "paragonie/sodium_compat": "*"
    }
}
